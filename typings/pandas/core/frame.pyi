    def bfill(self, *args: Any, **kwargs: Any) -> DataFrame:
        """
        Fill NA/NaN values by using the next valid observation to fill the gap.
        
        Parameters
        ----------
        axis : {0 or 'index'} for Series, {0 or 'index', 1 or 'columns'} for DataFrame
